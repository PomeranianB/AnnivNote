<main>

  <h3>全ユーザー一覧</h3>
  
  <ul>
    <% @users.each do |user| %>
        <li>
        <%= image_tag user.get_profile_image %>
        <%=link_to(admin_user_path(user.id)) do %>
          　<%= user.name %>
        <% end %>
        - <%= user.email %>
          - <%= link_to '退会させる', admin_user_path(user), method: :delete, data: { confirm: '本当に退会させますか？' }, class: "my-custum-link" %>
      </li>
    <% end %>
  </ul>

</main>